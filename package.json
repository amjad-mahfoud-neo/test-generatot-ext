{
  "name": "testgenerator",
  "displayName": "TestGenerator",
  "description": "One click generate test files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "testGenerator.helloWorld",
        "title": "ðŸ§ª - Generate Unit Test File"
      },
      {
        "command": "testGenerator.generateTest",
        "title": "ðŸ§ª - Generate Tests"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceLangId == typescript",
          "command": "testGenerator.helloWorld",
          "group": "TestGenerator"
        },
        {
          "when": "resourceLangId == typescript",
          "command": "testGenerator.generateTest",
          "group": "TestGenerator"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  }
}